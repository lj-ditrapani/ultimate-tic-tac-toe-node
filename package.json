{
  "name": "ultimate-tic-tac-toe",
  "version": "0.0.1",
  "description": "ultimate-tic-tac-toe",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fmt": "prettier --write \"src/**/*.ts\" *.json \"test/**/*.ts\"",
    "fmt-check": "prettier --check \"src/**/*.ts\" *.json \"test/**/*.ts\"",
    "tlint": "tslint --fix -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "tlint-check": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "npm run tlint && npm run fmt",
    "lint-check": "npm run tlint-check && npm run fmt-check",
    "tsc": "tsc",
    "all-pipeline": "npm run lint-check && tsc && npm test",
    "all": "npm run lint && tsc && npm test"
  },
  "author": "Jonathan Di Trapani",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.0.2",
    "@types/ws": "^6.0.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
